{
  "name": "auto",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chartist": "^0.11.0",
    "connect-mongo": "^2.0.1",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "fusioncharts": "^3.12.2",
    "mongoose": "^5.0.15",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
