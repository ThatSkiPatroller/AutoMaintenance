  "dependencies": {
    "@date-io/date-fns": "^2.10.11",
    "@material-ui/core": "^4.11.4",
    "@material-ui/pickers": "^3.3.10",
    "concurrently": "^6.2.0",
    "connect-mongo": "^4.4.1",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.13",
